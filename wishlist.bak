<script>
function menuleft(x){
	if($(".secondpic").hasClass("visible")){
		$(".secondpic").hide().removeClass("visible")
		$(".firstpic").show().addClass("visible")
		$(".featurettemenuleft").hide()
		$(".featurettemenuright").show()
	}
	else if($(".thirdpic").hasClass("visible")){
		$(".thirdpic").hide().removeClass("visible")
		$(".secondpic").show().addClass("visible")
		$(".featurettemenuright").show()
	}
	else if($(".fourthpic").hasClass("visible")){
		$(".fourthpic").hide().removeClass("visible")
		$(".thirdpic").show().addClass("visible")
		$(".featurettemenuright").show()
	}
}
function menuright(x){
	if($(".firstpic").hasClass("visible")){
		$(".firstpic").hide().removeClass("visible")
		$(".secondpic").show().addClass("visible")
		$(".featurettemenuleft").show()
		$(".featurettemenuright").hide()
